{
  "name": "dummypackagename",
  "version": "1.0.0",
  "description": "dummy description",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/darco2018/dummyrepo.git"
  },
  "author": "Dariusz Ustrzycki <ustrzycki@javacoder.cloud>",
  "license": "ISC",
  "keywords": [],
  "bugs": {
    "url": "https://github.com/darco2018/dummyrepo/issues"
  },
  "homepage": "http://www.javacoder.cloud",
  "main": "./src/app.js",
  "scripts": {
    "stylelint": "stylelint src/**/*.css --fix",
    "willbeprefixed": "autoprefixer --info",
    "postcss": "postcss src/**/*.css  --base src   --dir build-css --verbose",
    "browsers": "browserslist",
    "coverage": "browserslist --coverage=Pl",
    "doiuse": "npm run sass && doiuse --browsers '> 0.2% in Pl, Chrome >= 70, ios_saf >= 11, Safari >= 12, Edge >= 17, not Samsung < 8.2, not dead, not OperaMini all, not ie > 1, not ie_mob > 1' ./src/css/_custom.css",
    "sass": "node-sass ./src/css/_custom.scss ./src/css/_custom.css",
    "kill": "netstat -nlp | grep 8080",
    "stats": "webpack --profile --json > stats.json",
    "lint": "eslint --fix src/",
    "watch": "webpack -d --watch  --display-error-details",
    "start": "webpack-dev-server  --config webpack.dev.js --open --display-error-details",
    "build": "webpack --config webpack.prod.js --display-error-details && npm run resolveUrlsInCss",
    "resolveUrlsInCss": "node ./resolve-url.js  ./dist/public/stylesheets  main 'url(../'"
  },
  "dependencies": {
    "bootstrap": "^4.4.1",
    "express": "^4.17.1",
    "lodash": "^4.17.15",
    "popper.js": "^1.16.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.7",
    "@babel/core": "^7.7.7",
    "@babel/preset-env": "^7.7.7",
    "@babel/register": "^7.7.7",
    "acorn": "^6.4.0",
    "apache-server-configs": "^3.1.0",
    "archiver": "^3.1.1",
    "babel-loader": "^8.0.6",
    "browserlist": "^1.0.1",
    "caniuse": "^0.1.3",
    "clean-webpack-plugin": "^2.0.2",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^2.1.1",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-config-prettier": "^4.2.0",
    "eslint-loader": "^2.2.1",
    "eslint-plugin-html": "^5.0.3",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-mocha": "^5.3.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-react-hooks": "^1.7.0",
    "file-loader": "^3.0.1",
    "glob": "^7.1.6",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "jquery": "^3.4.1",
    "main.css": "1.0.0",
    "mini-css-extract-plugin": "^0.5.0",
    "mocha": "^5.2.0",
    "modernizr": "3.7.1",
    "node-sass": "^7.0.0",
    "normalize.css": "8.0.1",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-cli": "^6.1.3",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^1.19.1",
    "sass-loader": "^7.3.1",
    "ssri": "^6.0.1",
    "strip-json-comments": "^2.0.1",
    "style-loader": "^0.23.1",
    "stylelint": "^10.1.0",
    "stylelint-config-recommended": "^2.2.0",
    "stylelint-webpack-plugin": "^0.10.5",
    "url-loader": "^1.1.2",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.1",
    "webpack-merge": "^4.2.2"
  },
  "engines": {
    "node": ">=6",
    "npm": ">=3"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "browserslist": [
    "> 0.2% in Pl",
    "Chrome >= 70",
    "ios_saf >= 11",
    "Safari >= 12",
    "Edge >= 17",
    "not Samsung < 8.2",
    "not OperaMini all",
    "not ie > 1",
    "not ie_mob > 1",
    "not dead"
  ],
  "h5bp-configs": {
    "directories": {
      "archive": "archive",
      "dist": "dist",
      "src": "src",
      "test": "test"
    }
  },
  "files": [
    "LICENSE.txt",
    "package.json",
    "dist/",
    "README.md"
  ]
}
